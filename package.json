{
	"name": "vscode-abap",
	"displayName": "ABAP Syntax Highlighting",
	"description": "Syntax highlighting for ABAP",
	"version": "0.5.9",
	"publisher": "larshp",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Programming Languages"
	],
  "scripts": {
    "vscode:prepublish": "npm test",
    "test": "mocha --recursive --reporter progress test"
  },
	"keywords": [
		"abap"
	],
	"bugs": {
		"url": "https://github.com/larshp/vscode-abap/issuess"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/larshp/vscode-abap.git"
	},
	"icon": "img/icon.png",
	"contributes": {
		"languages": [
			{
				"id": "abap",
				"configuration": "./language-configuration.json",
				"aliases": [
					"ABAP",
					"abap"
				],
				"extensions": [
					".abap",
					".ABAP"
				]
			}
		],
		"grammars": [
			{
				"language": "abap",
				"scopeName": "source.abap",
				"path": "./syntaxes/abap.tmLanguage"
			}
		]
	},
	"devDependencies": {
		"chai": "^5.1.1",
		"mocha": "^10.4.0"
	}
}
